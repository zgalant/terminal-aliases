alias dp='ssh deploy@codehs.com'
alias segway='ssh deploy@codehs.com'
alias server='workon codehs; cd ~/codehs/codehs; ./manage.py runserver'
alias shell='workon codehs; cd ~/codehs/codehs; ./manage.py shell_plus'
alias setup='workon codehs; cd ~/codehs/codehs;'
alias gpull='git checkout master; git fetch upstream; git merge upstream/master; git status; git push origin master; pip install -r ../requirements.txt; ./manage.py migrate'

alias frozen-server='workon codehs; cd ~/codehs/codehs; USE_FROZEN=True ./manage.py runserver'
alias frozen-shell='workon codehs; cd ~/codehs/codehs; USE_FROZEN=True ./manage.py shell_plus'