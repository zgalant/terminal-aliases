alias g='git'

alias gs='git status'
alias gsh='git stash'
alias ga='git add'
alias gp='git push'
alias gc='git commit -am'
alias gch='git checkout'
alias gf='git fetch'
alias gb='git branch'
alias gd='git diff'
alias gdo='git diff origin/master master'
alias gdw='git diff --word-diff'
alias gl='git log --oneline --decorate'
alias gld='git log master..origin/master --oneline --decorate'
alias gln='git ls-files | xargs wc -l'

alias gpom='git push origin master'
alias gmom='git merge origin/master'
alias gpod='git push origin dev'
alias gmod='git merge origin/dev'
alias gpwm='git push web master'
alias gphm='git push heroku master'

alias gpo='git push origin'

alias gm='git merge'